{
  "presets": [["@babel/preset-typescript"]],
  "plugins": [
    ["@babel/plugin-transform-typescript", { "allowDeclareFields": true }],
    [
      "babel-plugin-ember-template-compilation",
      {
        "targetFormat": "hbs",
        "transforms": [],
