{
  "name": "ember-scoped-css-compat",
  "version": "10.2.2",
  "description": "classic broccoli build automatic setup of ember-scoped-css",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/soxhub/ember-scoped-css.git",
    "directory": "ember-scoped-css-compat"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:fix": "prettier . -w",
    "lint:prettier": "prettier . -c",
    "start": "ember serve"
  },
  "dependencies": {
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^5.0.0",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^1.1.2",
    "@nullvoxpopuli/eslint-configs": "^5.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.5.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.0",
    "ember-page-title": "^9.0.0",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-scoped-css": "workspace:*",
    "ember-source": "~6.5.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.0.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.1",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "webpack": "^5.98.0"
  },
  "peerDependencies": {
    "ember-scoped-css": ">=0.18.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-htmlbars",
      "ember-template-imports"
    ]
  },
  "volta": {
    "extends": "../package.json"
  }
}
